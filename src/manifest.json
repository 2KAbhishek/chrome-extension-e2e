{
  "manifest_version": 3,
  "name": "Counter Sidebar",
  "key": "",
  "version": "1.0",
  "description": "A simple counter in a sidebar",
  "permissions": ["sidePanel", "storage"],
  "action": {
    "default_title": "Open Counter Sidebar"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
